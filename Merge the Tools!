def merge_the_tools(string, k):
    # your code goes here
    for i in range(0,len(string),k):
        chunk = string[i:i+k]
        s = set()
        out = []
        for ch in chunk:
            if ch not in s:
                s.add(ch)
                out.append(ch)
        print(''.join(out))
        
        

if __name__ == '__main__':
    string, k = input(), int(input())
    merge_the_tools(string, k)
