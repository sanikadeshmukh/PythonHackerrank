import re

n = int(input().strip())

# 1) Must start with 4/5/6; total 16 digits; optional hyphens only as 4-4-4-4
card_pattern = re.compile(r'^[456]\d{3}(?:-?\d{4}){3}$')

# 2) No 4+ consecutive repeated digits (after removing hyphens)
repeat4_pattern = re.compile(r'(\d)\1{3}')

for _ in range(n):
    s = input().strip()
    if card_pattern.match(s) and not repeat4_pattern.search(s.replace('-', '')):
        print("Valid")
    else:
        print("Invalid")
